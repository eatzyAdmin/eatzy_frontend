{
  "name": "eatzy",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "proxy": "node proxy-server.js",
    "start:customer": "pnpm --filter customer dev --port 3000",
    "start:driver": "pnpm --filter driver dev --port 3001",
    "start:restaurant": "pnpm --filter restaurant dev --port 3002",
    "start:admin": "pnpm --filter admin dev --port 3003",
    "start:super-admin": "pnpm --filter super-admin dev --port 3004"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "http-proxy": "^1.18.1"
  }
}